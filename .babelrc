{
  "env": {
    "development": {
      "presets": [
        [
          "es2015",
          {
            "loose": true,
            "modules": "commonjs"
          }
        ],
        "stage-2",
        "react"
      ],
      "sourceMaps": "inline"
    },
    "test": {
      "presets": [
        [
          "es2015",
          {
            "loose": true,
            "modules": "commonjs"
          }
        ],
        "stage-2",
        "react"
      ],
      "sourceMaps": "inline"
    },
    "rollup": {
      "presets": [
        [
          "es2015",
          {
            "loose": true,
            "modules": false
          }
        ],
        "stage-2",
        "react"
      ],
      "plugins": [
        "external-helpers"
      ]
    }
  }
}
