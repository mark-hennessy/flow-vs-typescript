{
  "extends": "tslint-config-airbnb",
  "rules": {
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"
    ],
    "radix": false,
    "function-name": false,
    "no-increment-decrement": false
  }
}
