{
  "name": "flow-vs-typescript",
  "description": "A sandbox app to experiment with various JavaScript technologies.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mark-hennessy/flow-vs-typescript.git"
  },
  "keywords": [
    "flow",
    "typescript"
  ],
  "dependencies": {
    "babel-polyfill": "^6.23.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.12",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "documentation": "^4.0.0-beta.18",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.2",
    "flow-bin": "^0.42.0",
    "jest": "^19.0.2",
    "jsdoc-babel": "^0.3.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-uglify": "^1.0.1",
    "tslint": "^4.5.1",
    "tslint-config-airbnb": "^1.1.1",
    "typedoc": "^0.5.9",
    "typescript": "^2.2.1",
    "webpack": "^2.3.2"
  },
  "scripts": {
    "clean": "npm run clean:out && npm run clean:cov && npm run clean:docs",
    "clean:out": "rimraf out",
    "clean:cov": "rimraf coverage",
    "clean:docs": "rimraf docs",
    "flow:start": "flow; exit 0",
    "flow:stop": "flow stop",
    "eslint": "eslint src; exit 0",
    "eslint:changed": "git diff --name-only --diff-filter=ACMRTUB `git merge-base HEAD master` | grep '\\.js$' | xargs eslint --; exit 0",
    "tslint": "tslint --config config/tslint.json src/**/*.ts; exit 0",
    "test": "jest --config config/jestconfig.json",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage; exit 0",
    "babel": "babel src --out-dir out/babel; exit 0",
    "webpack": "webpack --config config/webpack.config.js; exit 0",
    "webpack:binary": "webpack --config config/webpack.binary.config.js; exit 0",
    "rollup": "BABEL_ENV=rollup rollup --config config/rollup.config.js; exit 0",
    "rollup:binary": "BABEL_ENV=rollup rollup --config config/rollup.binary.config.js; exit 0",
    "docs:js": "documentation build src/**/flow/*.js --format html --output docs/js",
    "docs:ts": "typedoc src/**/typescript/*.ts --out docs/ts"
  },
  "author": "Mark Hennessy <mhennessy116@gmail.com>",
  "license": "MIT"
}
